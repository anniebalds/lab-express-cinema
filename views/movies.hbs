<a href="/">Home</a>
<h1>All Movies</h1>

<div class="container">

    {{#each movies}}

        <div class="maincard">
            <img src="{{this.image}}" alt="{{this.title}}"/>
            <h2>{{this.title}}</h2>
            <a href="/movies/{{this._id}}">See more</a>
        </div>
    {{/each}}
</div>